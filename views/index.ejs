<%- include('partials/header') %>
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.6.3/dist/jquery.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js"></script>
    <div class="navbar">
        <a href="/home">Home</a>
        <a href="/article/new">Add-Product</a>
        <a href="/profile/User">Profile</a>
        <div class="search-container">
            <form action="/search" method="post">
                <input type="text" name="keyi" placeholder="Search...." required>
                <button type="submit">search</button>
            </form>
        </div>
    </div>

    <div class="heading2">
        <h1>Available-Product</h1>
    </div>

    <div class="blog">
        <% article.forEach(element=> { %>


            <div class="card">
                <img src="<%=element.productimg.url%>" alt="Denim Jeans" style="width:100%">
                <h5>
                    <%= element.title %>
                </h5>
                <p class="price">Price:<%= element.price %></p>
                <!-- <h5><i>CreatedBy:<%=element.createdname %></i></h5> -->
                <div class="buttons">
                     <a class="btn btn_succcess" href="/articleview/<%=element.slug %>">View More</a>
                </div>
               

            </div>


            <% }); %>
    </div>
    <div class="footer">
        <p>Â© Share-Thing 2023</p>
      </div>

    <%- include('partials/footer') %>